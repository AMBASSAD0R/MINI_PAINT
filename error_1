# splir Ğ² error_1
def ft_len(z):
    x = 0
    for i in z:
        x = x + 1
    return x


def ft_split(string):
    tmp = ''
    mass = []
    for ch in string:
        if ch == ' ' and tmp:
            mass.append(tmp)
            tmp = ''
        else:
            tmp += ch
    if tmp:
        mass.append(tmp)
    return mass


